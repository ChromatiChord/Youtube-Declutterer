{
  "name": "Youtube Player Declutterer",
  "version": "2.0",
  "manifest_version": 2,
  "icons": {
    "16": "./Declutter Icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Declutter"
  }
}
